    <tr style="height: 20px;">
      <th id="900176748R122" class="row-headers-background row-header-shim" style="z-index: 1; background: rgb(248, 249, 250); font-weight: normal; text-align: center; vertical-align: middle; font-size: 8pt; color: rgb(95, 99, 104); height: 20px; border-width: 0px 1px 1px 0px; overflow: hidden; padding: 0px; width: 0px; margin: 0px; position: relative; font-family: Roboto, RobotoDraft, Helvetica, Arial, sans-serif;">
